<project>

    <target name="clean">
        <delete dir="build"/>
    </target>

    <target name="compile">
        <mkdir dir="build/classes"/>
        <javac srcdir="src" destdir="build/classes"/>
    </target>

    <target name="jar" depends="compile,clean">
        <mkdir dir="build/jar"/>
        <jar destfile="build/jar/Test.jar" basedir="build/classes">
            <manifest>
                <attribute name="Main-Class" value="oata.Main"/>
            </manifest>
        </jar>
    </target>
	
</project>